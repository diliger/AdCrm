@{
    Layout = null;
}
<div id="divPaymentsDialog">
    <div class="separator"></div>
    <div class="box" data-bind="with: $root.selected.work">
        <fieldset class="collapsable">
            <legend>
                <span class="title">Платежи</span>
            </legend>
            <div class="fieldset" >
                <div id="divPaymentsDialogList">
                    @{Html.RenderPartial("PaymentsList");}
                </div>
                <div class="text-left">
                    <div class="separator"></div>
                    <input type="button" data-bind="click: $root.createPayment" class="edit" value="Добавить платеж" />
                </div>
            </div>
        </fieldset>
    </div>
</div>
